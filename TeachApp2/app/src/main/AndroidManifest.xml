<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teachapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.TeachApp">
        <activity android:name="com.example.teachapp.StartTestActivity"></activity>
        <activity android:name="com.example.teachapp.QuestionsActivity" />
        <activity android:name="com.example.teachapp.TestActivity" />
        <activity android:name="com.example.teachapp.SignUpActivity" />
        <activity android:name="com.example.teachapp.LoginActivity" />
        <activity android:name="com.example.teachapp.MainActivity" />
        <activity
            android:name="com.example.teachapp.splashActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.TeachApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>